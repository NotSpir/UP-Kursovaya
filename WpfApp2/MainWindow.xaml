<Window x:Class="WpfApp2.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp2"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="75"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="25"/>
        </Grid.RowDefinitions>
        <Frame Name="MainFrame" Grid.Row="1" NavigationUIVisibility="Hidden" ContentRendered="MainFrame_ContentLoaded"/>
        <Grid Grid.Row="0"  Background="DarkBlue"/>
        <Grid Grid.Row="2" Background="Black"/>
        <Button Name="GoBackButton" Content="Назад" HorizontalAlignment="Right" VerticalAlignment="Center" Height="35" Width="153" Click="GoBackButton_Click"/>
        <Button Name="LogOutButton" Content="Выйти" HorizontalAlignment="Right" Margin="0 0 160 0" VerticalAlignment="Center" Height="35" Width="153" Click="LogOutButton_Click"/>
    </Grid>
</Window>
